{
  "branches": [
    {
        name: 'release/0.0.x',
        range: '0.0.x',
        channel: 'release/0.0.x'
    },
    'main'
  ],
  "repositoryUrl": "https://github.com/cheqd/did-provider-cheqd.git",
  "debug": true,
  "plugins": [
    ["@semantic-release/github", {
      "assets": ["dist/**"]
      }],
    ["@semantic-release/commit-analyzer", {
    "releaseRules": [
        {
          "tag": "Breaking",
          "release": "minor"
        },
        {
          "tag": "breaking",
          "release": "minor"
        },
        {
          "subject": "!no-release:*",
          "release": "patch"
        }]
    }
}