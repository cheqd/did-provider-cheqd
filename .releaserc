{
    "branches": [
        {
            name: 'release/1.0.x',
        },
        {
            name: 'release/1.1.x',
        }
    ],
    "debug": "true",
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "eslint",
                "releaseRules": [
                   {
                     "tag": "Breaking",
                     "release": "minor"
                   },
                   {"tag": "New", "release": "patch"},
                   {"tag": "Update", "release": "patch"},
                ],
            }
        ],
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md"
            }
        ],
        [
            "@semantic-release/release-notes-generator",
            {
                "preset": "eslint"
            }

        ],

    ]
}