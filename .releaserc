{
  "branches": [
    {
        name: 'release/1.0.x',
        channel: 'release/1.0.x'
    },
    'main'
  ],
  "extends": ["@conventional-changelog-eslint"],
  "debug": "true",
  "plugins": [
    ["@semantic-release/github", {
      "assets": ["dist/**"]
      }],
    ["@semantic-release/commit-analyzer", {
        "preset": "eslint",
        "releaseRules": [
            {
              "tag": "Breaking",
              "release": "minor"
            },
            {"tag": "New", "release": "patch"},
            {"tag": "Fix", "release": "patch"},
        ],
     }],
    ["@semantic-release/npm"],
  ]
}